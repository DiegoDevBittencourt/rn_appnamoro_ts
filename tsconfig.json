// prettier-ignore
{
  "extends": "@tsconfig/react-native/tsconfig.json", /* Recommended React Native TSConfig base */
  "compilerOptions": {
    /* Visit https://aka.ms/tsconfig.json to read more about this file */
    /* Completeness */
    "strict": true, //dotenv
    "skipLibCheck": true, /* Skip type checking all .d.ts files. */
    "baseUrl": "src",
    "paths": {
      "~/*": [
        "*"
      ],
      "@assets/*": [
        "./assets/*"
      ],
      "@components/*": [
        "./components/*"
      ],
      "@config/*": [
        "./config/*"
      ],
      "@constants/*": [
        "./constants/*"
      ],
      "@database/*": [
        "./database/*"
      ],
      "@modals/*": [
        "./modals/*"
      ],
      "@routes/*": [
        "./routes/*"
      ],
      "@screens/*": [
        "./screens/*"
      ],
      "@store/*": [
        "./store/*"
      ],
      "@utils/*": [
        "./utils/*"
      ]
    }
  },
  "include": [
    "src/**/*"
  ],
  "types": [
    "jest",
    "styled-components-react-native"
  ],
  "typeRoots": [ //dotenv
    "./types"
  ] // <------ you need to add this
}